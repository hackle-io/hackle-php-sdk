{
    "name": "hackle/hackle-php-sdk",
    "description": "Hackle SDK for PHP",
    "keywords": [
        "hackle",
        "hackle sdk",
        "hackle php"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Hackle",
            "email": "platform@hackle.io"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4.5",
        "monolog/monolog": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Hackle\\": "src/Hackle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hackle\\Tests\\": "tests/"
        }
    }
}
